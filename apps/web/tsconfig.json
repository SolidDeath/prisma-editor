{
  "extends": "tsconfig/nextjs.json",
  "exclude": [
    "node_modules"
  ],
  "include": [
    ".eslintrc.cjs",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.cjs",
    "**/*.mjs",
    "../../prettier.config.cjs",
    ".next/types/**/*.ts"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./src/*"
      ]
    },
    "module": "esnext",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  }
}
